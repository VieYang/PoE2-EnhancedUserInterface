      <FemaleText />
    </Entry>
    <Entry>
      <ID>2542</ID>
      <DefaultText>Immunity: Intellect Afflictions</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2543</ID>
      <DefaultText>Resistance: Intellect Afflictions</DefaultText>
      <FemaleText />
    </Entry>
